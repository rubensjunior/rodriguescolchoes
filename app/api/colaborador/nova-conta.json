                "test": ""
              },
              {
                "name": ":P5",
                "type": "expression",
                "value": "{{$_POST.ano_veiculo}}",
                "test": ""
              },
              {
                "name": ":P6",
                "type": "expression",
                "value": "{{$_POST.cor_veiculo}}",
                "test": ""
              }
            ]
          }
        },
        "meta": [
          {
            "name": "identity",
            "type": "text"
          },
          {
            "name": "affected",
            "type": "number"
          }
        ]
      },
      {
        "name": "motorista",
        "module": "dbupdater",
