      },
      {
        "type": "text",
        "name": "colaborador"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "hash",
        "module": "crypto",
        "action": "passwordHash",
        "options": {
          "password": "{{$_POST.cpf}}",
          "algo": "argon2id"
        },
        "outputType": "text"
      },
      {
        "name": "colaborador",
        "module": "dbupdater",
        "action": "insert",
        "options": {
          "connection": "db",
          "sql": {
            "type": "insert",
            "values": [
              {
                "table": "colaboradores",
                "column": "nome_colaborador",
                "type": "text",
                "value": "{{$_POST.nome_completo}}"
              },
              {
                "table": "colaboradores",
                "column": "cpf_colaborador",
                "type": "text",
                "value": "{{$_POST.cpf}}"
              },
              {
                "table": "colaboradores",
                "column": "data_nascimento",
                "type": "date",
                "value": "{{$_POST.data_nascimento}}"
              },
              {
                "table": "colaboradores",
                "column": "email",
                "type": "text",
                "value": "{{$_POST.email}}"
              },
              {
                "table": "colaboradores",
                "column": "telefone",
                "type": "text",
                "value": "{{$_POST.telefone}}"
              },
              {
                "table": "colaboradores",
                "column": "password",
                "type": "text",
                "value": "{{hash}}"
              }
            ],
            "table": "colaboradores",
            "returning": "id_colaborador",
            "query": "insert into \"colaboradores\" (\"cpf_colaborador\", \"data_nascimento\", \"email\", \"nome_colaborador\", \"password\", \"telefone\") values (?, ?, ?, ?, ?, ?)",
            "params": [
              {
                "name": ":P1",
                "type": "expression",
                "value": "{{$_POST.nome_completo}}",
                "test": ""
              },
              {
                "name": ":P2",
                "type": "expression",
                "value": "{{$_POST.cpf}}",
                "test": ""
              },
              {
                "name": ":P3",
                "type": "expression",
                "value": "{{$_POST.data_nascimento}}",
                "test": ""
              },
              {
                "name": ":P4",
                "type": "expression",
                "value": "{{$_POST.email}}",
                "test": ""
              },
              {
                "name": ":P5",
                "type": "expression",
                "value": "{{$_POST.telefone}}",
                "test": ""
              },
              {
                "name": ":P6",
                "type": "expression",
                "value": "{{hash}}",
                "test": ""
              }
            ]
          }
        },
        "meta": [
          {
            "name": "identity",
            "type": "text"
          },
          {
            "name": "affected",
            "type": "number"
          }
        ]
      },
      {
        "name": "veiculo",
        "module": "dbupdater",
        "action": "insert",
        "options": {
          "connection": "db",
          "sql": {
            "type": "insert",
            "values": [
              {
                "table": "veiculos_colaboradores",
                "column": "tipo_veiculo",
                "type": "text",
                "value": "{{$_POST.tipo_veiculo}}"
              },
              {
                "table": "veiculos_colaboradores",
                "column": "placa_veiculo",
                "type": "text",
                "value": "{{$_POST.placa}}"
              },
              {
                "table": "veiculos_colaboradores",
                "column": "colaborador",
                "type": "text",
                "value": "{{colaborador.identity}}"
              },
              {
                "table": "veiculos_colaboradores",
                "column": "modelo_veiculo",
                "type": "text",
                "value": "{{$_POST.modelo_veiculo}}"
              },
              {
                "table": "veiculos_colaboradores",
