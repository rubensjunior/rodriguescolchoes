            <h3 class="mb-3">Meu QR Code</h3>
            <button id="btn1" class="btn w-100 btn-primary">Exibir QR Code</button>

        </div>
        <div class="col-12 p-3 rounded-3 text-center border">
            <i class="fas fa-car fa-2x mb-3"></i>
            <h3 class="mb-3">Meus ve√≠culos</h3>
            <select id="select1" class="form-select" dmx-bind:options="usuario_logado.data.query_colaborador.veiculos_colaboradores" optiontext="content.usuario_logado.data.query_colaborador.veiculos_colaboradores[0].modelo_veiculo+content.usuario_logado.data.query_colaborador.veiculos_colaboradores[0].placa_veiculo" optionvalue="content.usuario_logado.data.query_colaborador.veiculos_colaboradores[0].id_veiculo_colaborador">
